<Window x:Class="_0431RoutedEventTest_Tunneling.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_0431RoutedEventTest_Tunneling"
        mc:Ignorable="d"
        Title="Tunneling" Height="400" Width="400"
        Background="GreenYellow"
        PreviewMouseDown="Window_PreviewMouseDown"
>
    <Grid PreviewMouseDown="PreviewMouseDownGrid">
        <Button Margin="10,10,10,10" PreviewMouseDown="PreviewMouseDownButton">
            <Canvas Width="210" Height="210" Background="#FF5D5252"
                    PreviewMouseDown="PreviewMouseDownCanvas">
                <Ellipse Fill="#FFFF0000" Stroke="#FF000000" Width="100" Height="100"
                         Canvas.Left="56.678" Canvas.Top="60.012"
                         PreviewMouseDown="PreviewMouseDownEllipse"/>
            </Canvas>
        </Button>
    </Grid>
</Window>
